generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = "***"
}

model user {
  userId    Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  email     String
  password  String
}

model migration {
  revision           Int       @default(autoincrement()) @id
  name               String
  datamodel          String
  status             String
  applied            Int
  rolled_back        Int
  datamodel_steps    String
  database_migration String
  errors             String
  started_at         DateTime
  finished_at        DateTime?

  @@map("_migration")
}
